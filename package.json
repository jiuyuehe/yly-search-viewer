{
  "name": "@yly/search-viewer",
  "version": "1.0.0",
  "description": "Vue3 file reading and preview component library with SDK interface",
  "type": "module",
  "main": "./dist/yly-search-viewer.umd.js",
  "module": "./dist/yly-search-viewer.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/yly-search-viewer.es.js",
      "require": "./dist/yly-search-viewer.umd.js",
      "types": "./dist/types/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:lib": "vite build --mode lib",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit --skipLibCheck",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "keywords": [
    "vue3",
    "file-viewer",
    "pdf",
    "office",
    "image-preview",
    "video-player",
    "sdk",
    "typescript"
  ],
  "author": "jiuyuehe",
  "license": "MIT",
  "dependencies": {
    "vue": "^3.4.0",
    "element-plus": "^2.4.0",
    "@element-plus/icons-vue": "^2.3.0",
    "pinia": "^2.1.0",
    "axios": "^1.6.0",
    "pdfjs-dist": "^3.11.0",
    "@vue-office/docx": "^1.4.0",
    "@vue-office/excel": "^1.7.0",
    "@vue-office/pdf": "^2.0.0",
    "hls.js": "^1.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.0",
    "prettier": "^3.1.0",
    "typescript": "~5.3.0",
    "unplugin-auto-import": "^0.17.0",
    "unplugin-vue-components": "^0.25.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.0",
    "vue-tsc": "^1.8.0"
  }
}